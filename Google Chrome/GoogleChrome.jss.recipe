<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Google Chrome JSSImporter recipe.</string>
    <key>Identifier</key>
    <string>com.github.ooda-health-it.jss.GoogleChrome</string>
    <key>Input</key>
    <dict>
		<key>CATEGORY</key>
		<string>Productivity</string>
		<key>NAME</key>
		<string>Zoom</string>
		<key>PROD_NAME</key>
		<string>%NAME%</string>
		<key>PROD_UPDATE_GROUP_NAME</key>
		<string>%NAME%-prod-update-smart</string>
		<key>PROD_UPDATE_GROUP_TEMPLATE</key>
		<string>SmartGroupTemplate-prod.xml</string>
		<key>PROD_UPDATE_POLICY_TEMPLATE</key>
		<string>PolicyTemplate-prod.xml</string>
		<key>SELF_SERVICE_DESCRIPTION</key>
		<string></string>
		<key>SELF_SERVICE_DISPLAY_NAME</key>
		<string>%NAME%</string>
		<key>SELF_SERVICE_ICON</key>
		<string>%NAME%.png</string>
		<key>SELF_SERVICE_INSTALL_BUTTON</key>
		<string>Install %version%</string>
		<key>SELF_SERVICE_REINSTALL_BUTTON</key>
		<string>Install %version%</string>
		<key>STOP_IF_NO_JSS_UPLOAD</key>
		<false/>
		<key>TRIGGERONLY_POLICY_CATEGORY</key>
		<string>Triggered Installers</string>
		<key>TRIGGERONLY_POLICY_TEMPLATE</key>
		<string>PolicyTemplate-prod-triggeronly.xml</string>
		<key>TRIGGER_NAME</key>
		<string>%NAME%-install</string>
    <dict>
    <key>ParentRecipe</key>
    <string>com.github.ooda-health-it.pkg.googlechrome</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>category</key>
                <string>%CATEGORY%</string>
                <key>groups</key>
                <array>
                    <dict>
                        <key>name</key>
                        <string>%GROUP_NAME%</string>
                        <key>smart</key>
                        <true/>
                        <key>template_path</key>
                        <string>%PROD_UPDATE_GROUP_TEMPLATE%</string>
                    </dict>
                </array>
                <key>policy_template</key>
                <string>%POLICY_TEMPLATE_MAIN%</string>
                <key>prod_name</key>
                <string>%PROD_NAME%</string>
            </dict>
            <key>Processor</key>
            <string>JSSImporter</string>
        </dict>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>groups</key>
                <string>%GROUP_NAME%</string>
                <key>pkg_path</key>
                <string></string>
                <key>policy_category</key>
                <string>%CATEGORY%</string>
                <key>policy_template</key>
                <string>%POLICY_TEMPLATE_INSTALL%</string>
                <key>prod_name</key>
                <string>%PROD_NAME%</string>
                <key>self_service_description</key>
                <string>%SELF_SERVICE_DESCRIPTION%</string>
            </dict>
            <key>Processor</key>
            <string>JSSImporter</string>
        </dict>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>groups</key>
                <string>%GROUP_NAME%</string>
                <key>pkg_path</key>
                <string></string>
                <key>policy_category</key>
                <string>%CATEGORY%</string>
                <key>policy_template</key>
                <string>%POLICY_TEMPLATE_UPDATE%</string>
                <key>prod_name</key>
                <string>%PROD_NAME%</string>
                <key>self_service_description</key>
                <string>%SELF_SERVICE_DESCRIPTION%</string>
            </dict>
            <key>Processor</key>
            <string>JSSImporter</string>
        </dict>
    </array>
</dict>
</plist>